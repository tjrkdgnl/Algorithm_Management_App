<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ama.algorithmmanagement">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Application.AMAApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AlgorithmManagement"
        android:usesCleartextTraffic="true">

        <activity android:name=".Activity.kDefault.KDefaultActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Activity.kDefault.KCallAPIActivity" />

        <activity android:name=".Activity.kDefault.KCallSolvedAlgorithmAct" />

        <activity android:name=".Activity.kDefault.KLoginActivity" />

        <activity android:name=".Activity.kDefault.KDefaultFirebaseTest"/>

        <activity android:name=".Activity.kDefault.KSharedActivity"/>

        <activity android:name=".Activity.kDefault.IdeaActivity"/>

    </application>

</manifest>